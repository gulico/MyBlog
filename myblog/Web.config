<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>
<!--配置连接字符串
connectionString属性来获取或设置数据库连接字符串
Data Source服务器名
Initial Catalog数据库名
-->
  <connectionStrings>
    <add 
      name="connStr" 
      connectionString="Data Source=192.168.21.65;Initial Catalog=newsDB;User ID=Web;Password=web123;" 
      providerName="System.Data.SqlClient"/>
  </connectionStrings>

</configuration>
